                private void mostrarParteAdoptar() {
    
    Dashboard dashboard = new Dashboard();
    
 
    dashboard.ShouwJPanel(new Adoptar());
    
   
    dashboard.setVisible(true);
    dashboard.setLocationRelativeTo(null);
}
